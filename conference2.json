{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": " Average homeownership by age, for people born in 1950s",
      "anchor": "start",
      "subtitle": "Bourquin et al, 2022",
      "fontSize": "14",
      "subtitleFontSize": "12",
      "color": "#000000",
      "subtitleColor": "#000000"
    },
    "height": "250",
    "width": "400",
    "autosize": {"type": "fit", "contains": "padding", "resize": false},
    "background": "#ffffff",
    "view": {"fill": "#ffffff", "strokeOpacity": 0},
    "data": {
      "url": "https://raw.githubusercontent.com/elise-gd/elise-gd.github.io/refs/heads/main/1980s%20-%20Feuille%201.csv",
      "format": {"type": "csv"}
    },
    "mark": {"type": "line", "color": "#f4134d", "strokeWidth": "2"},
    "encoding": {
      "x": {
        "field": "Age",
        "type": "nominal",
        "axis": {"title": "", "titleColor": "#122B39", "grid": false}
      },
      "y": {
        "field": "Percentage",
        "type": "quantitative",
        "axis": {"title": "", "titleColor": "#122B39", "grid": false}
      }
    }
  }
